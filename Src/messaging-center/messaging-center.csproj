<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>messaging_center</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>messaging-center</Title>
    <Authors>Walfhand</Authors>
    <Description>A simple tool to manage events without going through delegates / events</Description>
    <PackageTags>eventManager; event; subscriber;sender;</PackageTags>
    <PackageProjectUrl>https://github.com/Walfhand/messaging-center</PackageProjectUrl>
	<Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="7.0.0" />
  </ItemGroup>

</Project>
